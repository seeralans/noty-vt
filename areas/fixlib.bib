@article{lindenbergetal2006,
  ids = {lindenbergetal2006a},
  title = {Chemical Kinetics beyond the Textbook: Fluctuations, Many-Particle Effects and Anomalous Dynamics},
  shorttitle = {Chemical Kinetics beyond the Textbook},
  author = {Lindenberg, Katja and Oshanin, Gleb and Tachiya, Masanori},
  year = 2006,
  month = feb,
  journal = {J. Phys.: Condens. Matter},
  volume = {19},
  number = {6},
  pages = {1},
  publisher = {IOP Publishing},
  issn = {0953-8984},
  doi = {10.1088/0953-8984/19/6/060301},
  urldate = {2022-08-29},
  abstract = {Processes involving concentrations (or finite numbers) of particles moving randomly in space and reacting either upon encounter or via distance-dependent reaction rates are ubiquitous in condensed matter physics. Some of the many examples are recombination of ions or holes and electrons, excitation energy migration and quenching, trapping of particles by other species, in energy minima of random potential landscapes or in dangling ends of complex structures, coagulation, polymerization, and growth of dendrites or nuclei of a new phase. These processes have been studied for a long time and descriptions of their kinetic behaviour can be found in virtually every textbook on chemical physics. The conventional textbook picture is, however, a bit outdated being based on mean-field approaches that discard fluctuations and many-particle effects. At best, random transport of particles is taken into account in terms of the heuristic renormalization of the reaction rates following the Smoluchowski approach, which is far from adequate. About three decades ago it was realized that in some systems with reactions and random transport, as in many other many-particle systems, many-particle effects and spatial fluctuations play an essential role and result in anomalous behaviour when compared to the standard textbook predictions. This may be, in some instances, fluctuation-induced decay of mean concentrations of particles, anomalously slow approach to equilibrium described by power laws in place of the exponential ones predicted by conventional chemical kinetics. In other cases, chemical equilibrium may not even be a proper thermodynamic equilibrium but rather a steady-state dependent on kinetic parameters. These effects become even more pronounced for reactions taking place in disordered media, open chemical systems with external input of particles or in systems with anomalous diffusion. Within the last three decades much effort has been devoted to the analysis of fluctuation-induced and many-particle phenomena in chemical kinetics. Numerous seminal contributions are, however, scattered among many journals and no attempt has been made so far to present a unified picture. In this special issue on `Chemical kinetics beyond the textbook: fluctuations, many-particle effects and anomalous dynamics' we attempt to present a general overview of different facets of the fluctuation-induced behaviour in systems with reactions and random transport. This special issue comprises 50 seminal works and reviews of many researchers world-wide working on different aspects of fluctuation phenomena in chemical, physical and biophysical systems, both theoretical and experimental. We believe that this special issue will become a point of reference and inspiration for many researchers and wish to thank all the contributing authors for their effort and support of our endeavour.},
  langid = {english},
  file = {/Users/seeralan/Zotero/storage/2JUPY8IX/meta.html}
}

