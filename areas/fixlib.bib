@article{assisetal2019,
  title = {Road {{Permeability Index}}: {{Evaluating}} the Heterogeneous Permeability of Roads for Wildlife Crossing},
  shorttitle = {Road {{Permeability Index}}},
  author = {Assis, Julia C. and Giacomini, Henrique C. and Ribeiro, Milton Cezar},
