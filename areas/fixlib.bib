@article{amroetal2000,
  title = {High-{{Resolution Atomic Force Microscopy Studies}} of the {{Escherichia}} Coli {{Outer Membrane}}:\, {{Structural Basis}} for {{Permeability}}},
  shorttitle = {High-{{Resolution Atomic Force Microscopy Studies}} of the {{Escherichia}} Coli {{Outer Membrane}}},
  author = {Amro, Nabil A. and Kotra, Lakshmi P. and {Wadu-Mesthrige}, Kapila and Bulychev, Alexy and Mobashery, Shahriar and Liu, Gang-yu},
  year = 2000,
