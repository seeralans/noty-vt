@article{sevrioukova2009,
  title = {Redox-{{Linked Conformational Dynamics}} in {{Apoptosis-Inducing Factor}}},
  author = {Sevrioukova, Irina F.},
  year = 2009,
  month = jul,
  journal = {Journal of Molecular Biology},
  volume = {390},
