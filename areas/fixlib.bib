@article{zhangetal2017,
  title = {Vehicle Effects on Human Stratum Corneum Absorption and Skin Penetration},
  author = {Zhang, Alissa and Jung, Eui-Chang and Zhu, Hanjiang and Zou, Ying and Hui, Xiaoying and Maibach, Howard},
  year = 2017,
