@article{shaebanietal2020,
  title = {Computational Models for Active Matter},
  author = {Shaebani, M. Reza and Wysocki, Adam and Winkler, Roland G. and Gompper, Gerhard and Rieger, Heiko},
  year = 2020,
  month = apr,
  journal = {Nat Rev Phys},
