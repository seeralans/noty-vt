@article{underhilldoyle2004,
  title = {On the Coarse-Graining of Polymers into Bead-Spring Chains},
  author = {Underhill, Patrick T. and Doyle, Patrick S.},
  year = 2004,
  month = sep,
  journal = {Journal of Non-Newtonian Fluid Mechanics},
