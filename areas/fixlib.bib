  author = {Thygesen, Uffe H{\o}gsbro and Pedersen, Martin W{\ae}ver and Madsen, Henrik},
  editor = {Nielsen, Jennifer L. and Arrizabalaga, Haritz and Fragoso, Nuno and Hobday, Alistair and Lutcavage, Molly and Sibert, John},
