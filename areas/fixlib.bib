@article{zhaoetal2022,
  title = {Super-{{Resolution Imaging}} of {{Highly Curved Membrane Structures}} in {{Giant Vesicles Encapsulating Molecular Condensates}}},
  author = {Zhao, Ziliang and Roy, Debjit and Steink{\"u}hler, Jan and Robinson, Tom and Lipowsky, Reinhard and Dimova, Rumiana},
  year = 2022,
  journal = {Adv. Mater.},
