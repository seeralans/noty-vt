@misc{biswasetal2024,
  title = {Tau Accelerates Tubulin Exchange in the Microtubule Lattice},
  author = {Biswas, Subham and Grover, Rahul and Reuther, Cordula and Poojari, Chetan S. and Shaebani, M. Reza and Gr{\"u}newald, Mona and Zablotsky, Amir and Hub, Jochen S. and Diez, Stefan and John, Karin and Schaedel, Laura},
