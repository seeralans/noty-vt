@article{sarvaharmanetal2019,
  title = {From {{Micro-to-Macro}}: {{How}} the {{Movement Statistics}} of {{Individual Walkers Affect}} the {{Formation}} of {{Segregated Territories}} in the {{Territorial Random Walk Model}}},
