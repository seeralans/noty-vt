@article{buseetal2007,
  title = {Modelling Habitat and Spatial Distribution of an Endangered Longhorn Beetle -- {{A}} Case Study for Saproxylic Insect Conservation},
  author = {Buse, J{\"o}rn and Schr{\"o}der, Boris and Assmann, Thorsten},
