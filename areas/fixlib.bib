@misc{liaoetal2023,
  title = {Theoretical Model of Confined Thermoviscous Flows for Artificial Cytoplasmic Streaming},
  author = {Liao, Weida and Erben, Elena and Kreysing, Moritz and Lauga, Eric},
  year = 2023,
