@misc{garcia-garciaetal2023,
  title = {Emergent {{Topology}} in {{Many-Body Dissipative Quantum Chaos}}},
  author = {{Garc{\'i}a-Garc{\'i}a}, Antonio M. and S{\'a}, Lucas and Verbaarschot, Jacobus J. M. and Yin, Can},
