@article{aengenheisteretal2018,
  title = {An Advanced Human in Vitro Co-Culture Model for Translocation Studies across the Placental Barrier},
  author = {Aengenheister, Leonie and Keevend, Kerda and Muoth, Carina and Sch{\"o}nenberger, Ren{\'e} and Diener, Liliane and Wick, Peter and {Buerki-Thurnherr}, Tina},
  year = 2018,
