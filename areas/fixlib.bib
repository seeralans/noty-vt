@article{lanoiseleeetal2018,
  ids = {lanoiseleeetal2018a,lanoiseleeetal2018b},
  title = {Diffusion-Limited Reactions in Dynamic Heterogeneous Media},
  author = {Lanoisel{\'e}e, Yann and Moutal, Nicolas and Grebenkov, Denis S.},
  year = 2018,
  month = oct,
  journal = {Nat Commun},
  volume = {9},
  number = {1},
